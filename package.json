{
  "name": "siderhub_2",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dev": "concurrently \"pnpm dev:backend\" \"pnpm dev:frontend\"",
    "dev:backend": "tsx watch src/backend/server.ts",
    "dev:frontend": "vite",
    "build": "pnpm build:frontend && pnpm build:backend",
    "build:frontend": "vite build",
    "build:backend": "tsc --project tsconfig.backend.json",
    "start": "node dist/backend/server.js",
    "preview": "vite preview",
    "typecheck": "tsc --noEmit",
    "test": "pnpm test:backend",
    "test:backend": "jest --config=jest.config.cjs",
    "test:backend:coverage": "jest --config=jest.config.cjs --coverage",
    "test:frontend": "vitest --config=vitest.config.ts",
    "test:frontend:coverage": "vitest --config=vitest.config.ts --coverage",
    "test:e2e": "playwright test",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:report": "playwright show-report coverage/e2e-report",
    "prisma:format": "prisma format",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev --name init",
    "prisma:seed": "tsx prisma/seed.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@prisma/client": "^6.18.0",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.5",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.9.2",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@types/supertest": "^6.0.3",
    "@types/testing-library__jest-dom": "^5.14.9",
    "@types/video.js": "^7.3.58",
    "@vitejs/plugin-react": "^5.1.0",
    "@vitest/coverage-v8": "^4.0.6",
    "express": "^5.1.0",
    "jest": "^29.7.0",
